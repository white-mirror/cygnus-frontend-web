{
  "name": "@cygnus/frontend-web",
  "private": true,
  "version": "0.1.4",
  "type": "module",
  "main": "electron/main.js",
  "description": "Interfaz web de Cygnus basada en React y Vite.",
  "author": "Nicol√°s Panaino",
  "buildNumber": "251111676",
  "scripts": {
    "dev": "vite",
    "host": "vite --host",
    "lint": "eslint . --max-warnings=0",
    "format": "prettier --check .",
    "build": "npm run build:app && node ./scripts/update-build-number.mjs",
    "build:app": "npm run build:web",
    "build:web": "tsc --noEmit && vite build",
    "build:native": "tsc --noEmit && vite build --mode native",
    "electron:build": "node ./scripts/build-electron.js",
    "electron:build:local": "npm run electron:build -- --variant local",
    "electron:build:prod": "npm run electron:build -- --variant prod",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@fortawesome/react-fontawesome": "^3.1.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "autoprefixer": "^10.4.20",
    "build-number-generator": "^3.0.0",
    "dotenv": "^16.4.5",
    "electron": "^39.0.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "postcss": "^8.4.49",
    "prettier": "^3.6.2",
    "semver": "^7.7.3",
    "tailwindcss": "^3.4.16",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7"
  }
}
